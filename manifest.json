{
    "manifest_version": 2,
    "name": "Сбор информации",
    "version": "1.0",
    "description": "Расширение для сбора информации и вывода в консоль",
    "permissions": [
        "activeTab"
    ],
    "browser_action": {
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ]
        }
    ]
}