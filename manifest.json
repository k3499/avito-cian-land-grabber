{
    "manifest_version": 2,
    "name": "Сбор информации с Avito/Cian",
    "version": "1.1",
    "description": "Расширение для сбора информации и вывода в консоль",
    "permissions": [
        "activeTab"
    ],
    "icons": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "browser_action": {
        "default_popup": "popup.html"
    },
    "all_frames": false,
    "content_scripts": [
        {
            "all_frames": false,
            "run_at": "document_end",
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ]
        }
    ]
}